{
	"name": "nuxt-app",
	"type": "module",
	"private": true,
	"packageManager": "pnpm@10.11.0",
	"scripts": {
		"build": "nuxt build",
		"dev": "nuxt dev --o",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"start": "node .output/server/index.mjs",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prettier": "prettier --write .",
		"reinstall": "rm -rf node_modules && rm -rf .nuxt && rm -f bun.lock && rm -f pnpm-lock.yaml && pnpm install",
		"svgo": "svgo -f app/assets/svg/original -o app/assets/svg/optimized",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{vue,js,ts,mjs,css,scss}": [
			"pnpm prettier",
			"pnpm lint:fix"
		]
	},
	"resolutions": {
		"ipx": "^3.0.0"
	},
	"dependencies": {
		"@nuxt/image": "^1.11.0",
		"@pinia/nuxt": "0.11.2",
		"date-fns": "^4.1.0",
		"nuxt": "^4.1.1",
		"pinia": "^3.0.3",
		"vue": "^3.5.20",
		"vue-router": "^4.5.1"
	},
	"devDependencies": {
		"@nuxt/eslint": "1.9.0",
		"@nuxtjs/tailwindcss": "^6.14.0",
		"@vue/runtime-core": "^3.5.21",
		"@vue/runtime-dom": "^3.5.21",
		"eslint": "^9.0.0",
		"eslint-plugin-prettier": "^5.5.4",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.6",
		"nuxt-svg-sprite-icon": "^2.0.1",
		"prettier": "^3.6.2",
		"prettier-plugin-organize-imports": "^4.2.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"sass": "^1.92.1",
		"svgo": "^4.0.0",
		"typescript": "^5.9.2",
		"vue-tsc": "^3.0.6"
	}
}
