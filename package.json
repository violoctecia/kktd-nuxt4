{
	"name": "nuxt-app",
	"type": "module",
	"private": true,
	"scripts": {
		"build": "nuxt build",
		"dev": "nuxt dev --o",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"start": "node .output/server/index.mjs",
		"-- linting --": "",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"-- formatting --": "",
		"prettier": "prettier --write .",
		"-- other --": "",
		"reinstall": "rm -rf node_modules && rm -rf .nuxt && rm -f bun.lock && rm -f pnpm-lock.yaml && bun install",
		"svgo": "svgo -f app/assets/svg/original -o app/assets/svg/optimized",
		"prepare": "husky"
	},
	"dependencies": {
		"@nuxt/image": "^1.11.0",
		"nuxt": "^4.1.1",
		"vue": "^3.5.20",
		"vue-router": "^4.5.1"
	},
	"devDependencies": {
		"@nuxt/eslint": "1.9.0",
		"@nuxtjs/tailwindcss": "^6.14.0",
		"eslint": "^9.0.0",
		"eslint-plugin-prettier": "^5.5.4",
		"husky": "^9.1.7",
		"nuxt-svg-sprite-icon": "^2.0.1",
		"prettier": "^3.6.2",
		"prettier-plugin-organize-imports": "^4.2.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"sass": "^1.92.1",
		"svgo": "^4.0.0",
		"typescript": "^5.9.2",
		"vue-tsc": "^3.0.6"
	},
	"packageManager": "bun@1.2.21"
}
